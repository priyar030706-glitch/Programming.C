/* Write a C program to store mobile details using structure */
#include <stdio.h>
struct Mobile {
    char brand[20];
    int model;
    float price;
};

int main() {
    struct Mobile m;
    scanf("%s %d %f", m.brand, &m.model, &m.price);
    printf("Brand: %s\nModel: %d\nPrice: %.2f", m.brand, m.model, m.price);
    return 0;
}
