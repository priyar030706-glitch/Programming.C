/* Program to count the number of records stored in an array of structures and display the count */
#include <stdio.h>
struct Item { int id; char name[20]; };
int main() {
    struct Item items[3];
    for(int i=0;i<3;i++) scanf("%d %s",&items[i].id,items[i].name);
    printf("Number of records = 3");
    return 0;
}

/* Input:
101 Pen
102 Book
103 Bag
Output:
Number of records = 3
*/
