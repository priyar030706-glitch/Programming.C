/* Write a C program to check whether a string is palindrome */
#include <stdio.h>
#include <string.h>
int main() {
    char s[50];
    int i, flag=1;
    scanf("%s", s);
    for(i=0;i<strlen(s)/2;i++)
        if(s[i]!=s[strlen(s)-1-i]){
            flag=0; break;
        }
    if(flag) printf("Palindrome");
    else printf("Not Palindrome");
    return 0;
}
