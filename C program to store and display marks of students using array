/* Write a C program to store and display marks of students using array */
#include <stdio.h>
int main() {
    int n, i, marks[50];
    printf("Enter number of students: ");
    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &marks[i]);
    printf("Marks:\n");
    for(i = 0; i < n; i++)
        printf("%d ", marks[i]);
    return 0;
}
