/* Program to store username and password using variables and verify login */
#include <stdio.h>
#include <string.h>
int main() {
    char user[20]="admin", pass[20]="123", u[20], p[20];
    scanf("%s %s", u, p);
    if(strcmp(u,user)==0 && strcmp(p,pass)==0) printf("Login Successful");
    else printf("Login Failed");
    return 0;
}

/* Input:
admin 123
Output:
Login Successful
*/
